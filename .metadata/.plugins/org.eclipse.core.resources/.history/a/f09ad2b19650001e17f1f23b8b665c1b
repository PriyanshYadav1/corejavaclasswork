package P3;

import P1.Mobile;
import P2.laptop;

public class main {
	public static void main(String args[])
	{
		int choice = 0;
		System.out.println("Enter 1 for mobile, 2 for Laptop or 0 to exit");
		switch (choice) 
		{
		case 0:
			break;
		case 1:
		{
			Mobile m1 = new Mobile();
			m1.disp();
			Mobile m2 = new Mobile();
			m2.accept();
			m2.disp();
		}
		case 2:
		{
			laptop m1 = new laptop();
			m1.disp();
			laptop m2 = new laptop();
			m2.accept();
			m2.disp();
		}
		}
			
	}
}
