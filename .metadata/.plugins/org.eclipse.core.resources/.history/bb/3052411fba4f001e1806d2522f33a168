package com.java.initial_project.service;

public class Mobile
{
	private String name,manufacturer,os;
	private float price;
	private int ramsize;
	
	public Mobile()
	{
		this("Genric","Unknown","OS",0f,0);
	}
	
	public Mobile(String name, String manufacturer,String os,float price, int ramsize)
	{
	this.name = name;
	this.manufacturer = manufacturer;
	this.price = price;
	this.os=os;
	this.ramsize=ramsize;
	}
	
	}